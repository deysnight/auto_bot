{
  "name": "auto-bot-front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "ng:start": "ng serve",
    "ng:build": "ng build",
    "build-opencv": "npx opencv-build-npm && npx build-opencv --version 4.6.0 rebuild",
    "start:back:dev": "cross-env NODE_ENV=development npx nodemon",
    "build": "rimraf ./build && ng build && tsc",
    "start:dev": "cross-env NODE_ENV=development npm run build && node build/src/index.js",
    "start": "cross-env NODE_ENV=production npm run build && node build/src/index.js"
  },
  "private": true,
  "type": "module",
  "opencv4nodejs": {
    "autoBuildOpencvVersion": "4.6.0"
  },
