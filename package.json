    "@angular/core": "^17.3.6",
    "@angular/forms": "^17.3.6",
    "@angular/material": "^17.3.6",
    "@angular/material-experimental": "^17.3.6",
    "@angular/platform-browser": "^17.3.6",
    "@angular/platform-browser-dynamic": "^17.3.6",
    "@angular/router": "^17.3.6",
    "@u4/opencv4nodejs": "^6.5.3",
    "cron-parser": "^4.9.0",
    "cronstrue": "^2.49.0",
    "dotenv": "^16.4.5",
